    "k=3\n",
    "m=2\n",
    "a=[1,0,0,1,0,1,0,1,1]\n",
    "i,j=(0,k-1)\n",
    "count=0\n",
    "while(i<=j and j<(len(a))):\n",
    "    sc=0\n",
    "    for index in range(i,j+1):\n",
    "        if a[index]==1:\n",
    "            sc+=1\n",
    "    if sc==m:\n",
    "        count+=1\n",
    "    i+=1\n",
    "    j+=1\n",
    "print(count)"
   ]
